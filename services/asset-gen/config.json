{
  "service": {
    "name": "GameForge SDXL Service",
    "version": "1.0.0",
    "port": 8000,
    "host": "0.0.0.0"
  },
  "model": {
    "name": "stable-diffusion-xl-base-1.0",
    "path": "/app/models/stable-diffusion-xl-base-1.0",
    "fallback": "stabilityai/stable-diffusion-xl-base-1.0",
    "precision": "fp16",
    "enable_xformers": true
  },
  "generation": {
    "default_width": 1024,
    "default_height": 1024,
    "default_steps": 20,
    "default_guidance_scale": 7.5,
    "max_width": 2048,
    "max_height": 2048,
    "max_steps": 50
  },
  "gpu": {
    "enable_autocast": true,
    "memory_efficient_attention": true,
    "cuda_device": 0
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}
